{
  "name": "@zalter/http2-client-node",
  "author": "Zalter",
  "version": "1.1.0",
  "private": false,
  "license": "MIT",
  "description": "A thin layer over Node.js HTTP/2 client",
  "type": "module",
  "scripts": {
    "clean": "rimraf lib",
    "build": "npm run clean && rollup --config rollup.config.js"
  },
  "repository": {
    "url": "https://github.com/zalter-com/http2-client-node.git"
  },
  "keywords": [
    "http2",
    "h2",
    "client",
    "node",
    "fetch",
    "session",
    "request",
    "connection",
    "stream",
    "data",
    "zalter"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.mjs"
    }
  },
  "types": "./lib/index.d.ts",
  "module": "./lib/index.mjs",
  "files": [
    "lib",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/node": "^18.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.76.0",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.4.0"
  }
}
